"projects/music-master/components/App.js","original":{"line":41,"column":29}},{"generated":{"line":68,"column":24},"source":"projects/music-master/components/App.js","original":{"line":41,"column":24}},{"name":"tracks","generated":{"line":68,"column":25},"source":"projects/music-master/components/App.js","original":{"line":41,"column":35}},{"generated":{"line":69,"column":0},"source":"projects/music-master/components/App.js","original":{"line":41,"column":8}},{"generated":{"line":69,"column":6},"source":"projects/music-master/components/App.js","original":{"line":35,"column":6}},{"generated":{"line":69,"column":7},"source":"projects/music-master/components/App.js","original":{"line":34,"column":4}},{"generated":{"line":70,"column":0},"source":"projects/music-master/components/App.js","original":{"line":44,"column":3}},{"generated":{"line":72,"column":0},"source":"projects/music-master/components/App.js","original":{"line":8,"column":28}},{"name":"App","generated":{"line":74,"column":15},"source":"projects/music-master/components/App.js","original":{"line":47,"column":15}}],"sources":{"projects/music-master/components/App.js":"import React, { Component } from 'react';\nimport Search from './Search';\nimport Artist from './Artist';\nimport Tracks from './Tracks';\n\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\n\nclass App extends Component {\n  state = { artist: null, tracks: [] };\n\n  componentDidMount() {\n    this.searchArtist('bts');\n  }\n\n  searchArtist = artistQuery => {\n    fetch(`${API_ADDRESS}/artist/${artistQuery}`)\n      .then(response => response.json())\n      .then(json => {\n        if (json.artists.total > 0) {\n          const artist = json.artists.items[0];\n\n          this.setState({ artist });\n\n          fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n            .then(response => response.json())\n            .then(json => this.setState({ tracks: json.tracks }))\n            .catch(error => alert(error.message));\n        }\n      })\n      .catch(error => alert(error.message));\n  };\n\n  render() {\n    return (\n      <div>\n        <h2>Music for Relax</h2>\n        <p style={{color: 'red'}}>Sorry. Music is just previews for 30 seconds.</p>\n        <p style={{color: 'blue'}}>Find your favorite singer and <em>play it!</em></p>\n        <Search searchArtist={this.searchArtist} />\n        <Artist artist={this.state.artist} />\n        <Tracks tracks={this.state.tracks} />\n      </div>\n    );\n  }\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"88f8c0e3fc8107a9b2c0eecc4c79308c","cacheData":{"env":{}}}